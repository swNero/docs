{
  "openapi": "3.0.1",
  "info": {
    "title": "msp-weather-service",
    "description": "A service for fetching weather data at given location",
    "contact": {
      "name": "P3 digital services GmbH",
      "email": "contact@energystacks.io"
    },
    "version": "v1"
  },
  "servers": [
    {
      "url": "https://p3-acc.energystacks.io/msp-weather/v1",
      "description": "Generated server url"
    }
  ],
  "paths": {
    "/weather": {
      "post": {
        "tags": [
          "Weather Controller"
        ],
        "summary": "Get weather data at given datetime for given location",
        "operationId": "getWeatherForecast",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/WeatherForecastRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Operation"
          },
          "404": {
            "description": "Not found"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LatLng": {
        "type": "object",
        "properties": {
          "lat": {
            "type": "number",
            "format": "double"
          },
          "lng": {
            "type": "number",
            "format": "double"
          }
        }
      },
      "WeatherForecastRequest": {
        "required": [
          "coordinate",
          "dateTime"
        ],
        "type": "object",
        "properties": {
          "coordinate": {
            "$ref": "#/components/schemas/LatLng"
          },
          "dateTime": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    },
